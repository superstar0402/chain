{
  "name": "proto",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/proto/src",
  "projectType": "library",
  "targets": {
    "build:js": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "outputs": ["{projectRoot}/main"]
    },
    "build:ts": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:ts"
      },
      "outputs": ["{projectRoot}/types"]
    },
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "buidl || true"
      },
      "outputs": ["{projectRoot}/main", "{projectRoot}/types"],
      "dependsOn": ["codegen"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs proto {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "nx:run-script",
      "options": {
        "script": "lint"
      }
    },
    "test": {
      "executor": "nx:run-script",
      "options": {
        "script": "test"
      }
    },
    "codegen": {
      "executor": "nx:run-script",
      "options": {
        "script": "codegen"
      }
    }
  },
  "tags": []
}
