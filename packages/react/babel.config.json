{
  "env": {
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs"
        }],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime"
      ]
    },
    "esm": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime"
      ]
    }
  }
}
